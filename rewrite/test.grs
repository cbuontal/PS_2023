package correction {
  rule lemma_leur {
    pattern { N [form="leur", upos=PRON] }
    without { N.lemma = lui }
    commands { N.lemma = lui }
  }
  
  # Clément BUON
  rule corriger_upos_etre {
  pattern { AUXILIAIRE [lemma="être", upos=AUX] }
  without {
      VERBE [upos=VERB];
      AUXILIAIRE -[comp:aux]-> VERBE;
  }
  commands {
      AUXILIAIRE.upos =  VERB;
  }
  }
}

strat main {
  Onf (correction)
}
